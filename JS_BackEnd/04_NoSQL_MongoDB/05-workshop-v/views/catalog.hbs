<section>
<h1>All Accomodation</h1>
<div class="search">
    <h4>Search</h4>
    <form method="get">
       
    <input name="search" value="{{search}}">
    <label for="city">City
        <input type="text" name="city" value="{{city}}">
    </label>
    <input type="submit"value="Search">
    </form>
</div>
<main>
    {{#if rooms}}
    <ul>
        {{#each rooms}}
        {{>roomCard}}
        {{/each}}
    </ul>
    {{else}}
    <p>Nothing here yet</p>
    {{/if}}
</main>

</section>