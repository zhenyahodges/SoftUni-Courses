<section>
<h1>All Accomodation</h1>
<div class="search">
    <p>Search</p>
    <form method="get">
    <input type="text" name="search">
    <input type="submit"value="Search">
    </form>
</div>
<main>
    {{#if rooms}}
    <ul>
        {{#each rooms}}
        {{>roomCard}}
        {{/each}}
    </ul>
    {{else}}
    <p>Nothing here yet</p>
    {{/if}}
</main>

</section>